{
    "manifest_version": 3,
    "name": "Zee Social",
    "description": "Hamza Wazir Social media links",
    "version": "1.0.0",
    "icons":{"128": "zee_28.png"},
    "action": {
        "default_icon": "zee.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["popup.js"]
    }
    ],
    "permissions": ["activeTab"]
    
}